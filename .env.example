# Copy to .env and fill values
PORT=4000
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/nerdalert?retryWrites=true&w=majority
MONGO_DB_NAME=nerdalert
# HuggingFace API Key (get from https://huggingface.co/settings/tokens)
# HuggingFace API Key (get from https://huggingface.co/settings/tokens)
HF_API_KEY=YOUR_HF_API_KEY_HERE
# HuggingFace Models
HF_TEXT_MODEL=mistralai/Mistral-7B-Instruct-v0.2
HF_EMBEDDING_MODEL=sentence-transformers/all-mpnet-base-v2
HF_MAX_TOKENS=450
